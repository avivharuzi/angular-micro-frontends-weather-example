{
  "version": 2,
  "projects": {
    "forecast": "apps/forecast",
    "forecast-data-access": "libs/forecast/data-access",
    "forecast-e2e": "apps/forecast-e2e",
    "weather": "apps/weather",
    "weather-api": "apps/weather-api",
    "weather-e2e": "apps/weather-e2e"
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "generators": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "weather"
}
