{
  "projectType": "library",
  "root": "libs/forecast/feature-main",
  "sourceRoot": "libs/forecast/feature-main/src",
  "prefix": "forecast",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/forecast/feature-main"],
      "options": {
        "jestConfig": "libs/forecast/feature-main/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/forecast/feature-main/src/**/*.ts",
          "libs/forecast/feature-main/src/**/*.html"
        ]
      }
    }
  },
  "tags": ["scope:forecast", "type:feature"]
}
